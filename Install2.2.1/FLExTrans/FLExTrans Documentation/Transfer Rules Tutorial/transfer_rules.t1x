<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE transfer PUBLIC "-//XMLmind//DTD transfer//EN"
"transfer.dtd">
<transfer
><section-def-cats
><def-cat
n="some_word_category"
><cat-item
tags="mytag.*"
></cat-item
></def-cat
></section-def-cats
><section-def-attrs
><def-attr
n="word_feature"
><attr-item
tags="myfeature"
></attr-item
><attr-item
tags="myotherfeature"
></attr-item
></def-attr
></section-def-attrs
><section-def-vars
><def-var
n="blank"
></def-var
></section-def-vars
><section-rules
><rule
comment="comment goes here"
><pattern
><pattern-item
n="some_word_category"
></pattern-item
></pattern
><action
><let
><clip
part="word_feature"
pos="1"
side="sl"
></clip
><lit-tag
v="myotherfeature"
></lit-tag
></let
><out
><lu
><clip
part="whole"
pos="1"
side="tl"
></clip
></lu
></out
></action
></rule
></section-rules
></transfer
>

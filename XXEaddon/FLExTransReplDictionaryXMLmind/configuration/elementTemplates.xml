<?xml version="1.0" encoding="UTF-8"?>
<cfg:configuration name="FLExTrans" xmlns:xslt="http://www.w3.org/1999/XSL/Transform" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:cfg="http://www.xmlmind.com/xmleditor/schema/configuration">
  <!--
========================================
   Element templates 
========================================
-->
  <cfg:elementTemplate name="space">
    <b></b>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="entry_pair">
    <e><p><l><leftdata>SourceWord1.1</leftdata><s n="cat99"/></l><r><rightdata>TargetWord1.1</rightdata><s n="cat99"/></r></p></e>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="identity">
    <i>SourceWord1.1<s n="cat99"/></i>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="left">
    <l><leftdata>SourceWord1.1</leftdata><s n="cat99"/></l>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="pair">
    <p><l><leftdata>SourceWord1.1</leftdata><s n="cat99"/></l><r><rightdata>TargetWord1.1</rightdata><s n="cat99"/></r></p>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="right">
    <r><rightdata>TargetWord1.1</rightdata><s n="cat99"/></r>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="section">
    <section id="replacement" type="standard">
      <e><p><l><leftdata>SourceWord1.1</leftdata><s n="cat99"/></l><r><rightdata>TargetWord1.1</rightdata><s n="cat99"/></r></p></e> 
    </section>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="category-or-affix">
    <s n="cat99"></s>
  </cfg:elementTemplate>
  <cfg:elementTemplate name="category-definition">
    <sdef n="cat99" c="Category/Affix description"></sdef>
  </cfg:elementTemplate>
</cfg:configuration>